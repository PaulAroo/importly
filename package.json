{
	"name": "importly",
	"version": "0.1.0-dev.0",
	"description": "concept web package manager",
	"license": "ISC",
	"type": "module",
	"files": [
		"x",
		"s"
	],
	"bin": {
		"importly": "x/cli.js"
	},
	"scripts": {
		"build": "run-s clean compile-typescript test",
		"clean": "rm -rf x",
		"compile-typescript": "tsc",
		"watch": "npm run -s compile-typescript -- -w",
		"test": "node x/cli.js --lookup=jsdelivr --host=unpkg < test-input.json5 > x/importmap.example.json",
		"start": "serve"
	},
