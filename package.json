		"@types/node": "^16.7.1",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
